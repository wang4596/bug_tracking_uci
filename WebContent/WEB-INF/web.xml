<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>BugTracker</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>LoginPage.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>BugTrackerInitServlet</servlet-name>
    <servlet-class>com.action.BugTrackerInitServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet>
    <description></description>
    <display-name>RegisterUserServlet</display-name>
    <servlet-name>RegisterUserServlet</servlet-name>
    <servlet-class>com.servlet.RegisterUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterUserServlet</servlet-name>
    <url-pattern>/RegisterUserServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>ForgetPassServlet</display-name>
    <servlet-name>ForgetPassServlet</servlet-name>
    <servlet-class>com.servlet.ForgetPassServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ForgetPassServlet</servlet-name>
    <url-pattern>/ForgetPassServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>createTicketServlet</display-name>
    <servlet-name>createTicketServlet</servlet-name>
    <servlet-class>com.servlet.createTicketServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>createTicketServlet</servlet-name>
    <url-pattern>/createTicketServlet</url-pattern>
  </servlet-mapping>
</web-app>